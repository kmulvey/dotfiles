[user]
    name = Kevin Mulvey
    email = kmulvey@linux.com
[alias]
    undo=reset --hard
    a=add --all
    c=commit -S -m
    co=checkout
    s=status
    l=log --show-signature
[color]
    ui = true
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[push]
    default = matching
[gpg]
    program = gpg2
[core]
    attributesfile = /home/fedora/.gitattributes
[commit]
    gpgsign = true
[init]
	defaultBranch = main
